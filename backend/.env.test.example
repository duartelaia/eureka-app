# PostgreSQL credentials
POSTGRES_USER=<your_db_username>
POSTGRES_PASSWORD=<your_db_password>
POSTGRES_DB=<your_db_name>

# Full PostgreSQL connection string
DATABASE_URL=postgresql://<your_db_username>:<your_db_password>@<db_host>:<port>/<your_db_name>

# Backend server port
BACKEND_PORT=<your_backend_port>

# JWT authentication settings
JWT_SECRET=<your_jwt_secret>
JWT_EXPIRES_DAYS=<days_token_remains_valid>

# Frontend origin (used for CORS or cookies)
FRONTEND_URL=http://<your_frontend_host>:<frontend_port>
